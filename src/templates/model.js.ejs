import Mongoose from 'mongoose';

const <%= props.name %>Model = new Mongoose.Schema({
    <%_ props.fields.forEach(field => { %>
        <%= field.fieldName %>: {
            type: <%= field.type %>, 
            require: true 
        },
    <% }) %>
},
{
    timestamps: true
});

export default Mongoose.model('<%= props.name %>Model', <%= props.name %>Model);
