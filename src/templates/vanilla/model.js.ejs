const Mongoose = require('mongoose')

const <%= props.name %>Model = new Mongoose.Schema({
    <% props.fields.forEach(field => { -%>
    <%= field.fieldName %>: {
            type: <%= field.type %>, 
            require: true 
        },
    <% }) -%>
},
    {
        timestamps: true
    }
)

module.exports = Mongoose.model('<%= props.name %>Model', <%= props.name %>Model)
